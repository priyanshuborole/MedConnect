<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_nav_graph"
    app:startDestination="@id/patientLoginFragment">


    <fragment
        android:id="@+id/doctorLoginFragment"
        android:name="com.priyanshudev.login.presentation.doctor.DoctorLoginFragment"
        android:label="fragment_doctor_login"
        tools:layout="@layout/fragment_doctor_login" >
        <action
            android:id="@+id/action_doctorLoginFragment_to_doctorProfileFragment"
            app:destination="@id/doctorProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/doctorProfileFragment"
        android:name="com.priyanshudev.login.presentation.doctor.DoctorProfileFragment"
        android:label="fragment_doctor_profile"
        tools:layout="@layout/fragment_doctor_profile" />
    <fragment
        android:id="@+id/patientLoginFragment"
        android:name="com.priyanshudev.login.presentation.patient.PatientLoginFragment"
        android:label="fragment_patient_login"
        tools:layout="@layout/fragment_patient_login" >
        <action
            android:id="@+id/action_patientLoginFragment_to_patientProfileFragment"
            app:destination="@id/patientProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/patientProfileFragment"
        android:name="com.priyanshudev.login.presentation.patient.PatientProfileFragment"
        android:label="fragment_patient_profile"
        tools:layout="@layout/fragment_patient_profile" />
</navigation>